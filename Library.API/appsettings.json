{
  "jwt": {
    "secretKey": "${SECRET_KEY}",
    "issuer": "${ISSUER}",
    "audience": "${AUDIENCE}"
  },
  "ConnectionStrings": {
    "default": "server=${SERVER};port=${PORT};database=${DATABASE};user=${USER};password=${PASSWORD};Persist Security Info=False; Connect Timeout=300"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
